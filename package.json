{
  "name": "mobile-home-park-manager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "cd Backend && python -m uvicorn app.main:app --host 127.0.0.1 --port 8000",
    "start": "concurrently \"npm run dev:backend\" \"npm run dev\" --names \"backend,frontend\" --prefix-colors \"blue,green\"",
    "build": "vite build",
    "preview": "vite preview",
    "generate:types": "openapi-typescript http://localhost:8000/openapi.json --output src/types/api.d.ts",
    "generate:types:local": "openapi-typescript Backend/openapi.json --output src/types/api.d.ts"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.42.0",
    "axios": "^1.13.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.1.0",
    "openapi-typescript": "^6.7.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
